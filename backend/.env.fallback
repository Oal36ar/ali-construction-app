
# Fallback Supabase Configuration
# Use this if direct PostgreSQL connection fails

# Set this to True to use REST API only
USE_REST_API_ONLY=True

# Disable direct PostgreSQL connection
DISABLE_POSTGRES_CONNECTION=True

# Connection retry settings
CONNECTION_RETRY_ATTEMPTS=3
CONNECTION_RETRY_DELAY=10

# Health check settings
HEALTH_CHECK_INTERVAL=300
HEALTH_CHECK_TIMEOUT=30
